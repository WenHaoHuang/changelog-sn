{
    "name": "changelog-sn",
    "version": "2.0.1",
    "author": "WenHaoHuang",
    "description": "changelog",
    "main": "lib/log/index.js",
    "module": "lib/log/index.js",
    "bin": {
        "changelog-sn": "./lib/core/cli.js"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "cz": "git add . && git cz",
        "log": "changelog-sn -i CHANGELOG.md -s -r 2",
        "log:all": "changelog-sn --config lib/all -i CHANGELOG.md -s -u -r 0",
        "v": "standard-version"
    },
    "keywords": [
        "changelog",
        "git"
    ],
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": ".cz-config.js"
        },
        "changelog-sn": {
            "jira": true,
            "type": "DASP",
            "url": "http://newjira.cnsuning.com/brower",
            "author": true,
            "dateFormat": "yyyy-mm-dd HH:MM:ss"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "standard-version":
    {
        "message": "chore(release): %s",
        "skip":
        {
            "changelog": true
        }
    },
    "dependencies": {
        "add-stream": "^1.0.0",
        "chalk": "^2.4.2",
        "compare-func": "^1.3.2",
        "longest": "^2.0.1",
        "q": "^1.5.1",
        "tempfile": "^3.0.0",
        "@commitlint/cli": "8.2.0",
        "commitizen": "4.0.3",
        "cz-customizable": "6.2.0",
        "github-url-from-git": "^1.5.0",
        "husky": "3.0.9"
    },
    "devDependencies": {
        "standard-version": "^7.0.0",
        "changelog-sn": "2.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/WenHaoHuang/changelog-sn"
    },
    "license": "MIT"
}
