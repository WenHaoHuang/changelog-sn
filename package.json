{
    "name": "changelog-sn",
    "version": "0.0.2",
    "author": "WenHaoHuang",
    "description": "changelog",
    "main": "dist/index.js",
    "module": "es/index.js",
    "files": [
        "lib"
    ],
    "scripts":
    {
        "cz": "git add . && git cz",
        "log": "conventional-changelog --config lib/log -i CHANGELOG.md -s -r 0",
        "v": "standard-version"
    },
    "keywords": [
        "changelog",
        "git"
    ],
    "config":
    {
        "commitizen":
        {
            "path": "./lib/cz"
        }
    },
    "husky":
    {
        "hooks":
        {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged":
    {
        "*.js": [
            "eslint --fix",
            "git add"
        ]
    },
    "dependencies":
    {
        "work-wrap": "^1.0.0",
        "q": "^1.5.1",
        "right-pad": "^1.0.1",
        "compare-func": "^1.3.2",
        "chalk": "^2.4.2",
        "longest": "^2.0.1"
    },
    "devDependencies":
    {
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "commitizen": "^4.0.3",
        "commitlint": "^8.1.0",
        "conventional-changelog-cli": "^2.0.23",
        "cross-env": "^5.2.0",
        "eslint": "^6.2.2",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.3",
        "github-url-from-git": "^1.5.0",
        "husky": "^3.0.4",
        "lint-staged": "^9.2.3",
        "standard-version": "^7.0.0"
    },
    "repository":
    {
        "type": "git",
        "url": "https://github.com/WenHaoHuang/changelog-sn"
    },
    "license": "MIT"
}
