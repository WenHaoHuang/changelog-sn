{
    "name": "changelog-sn",
    "version": "1.2.0",
    "author": "WenHaoHuang",
    "description": "changelog",
    "main": "lib/log/index.js",
    "module": "lib/log/index.js",
    "bin":
    {
        "changelog-sn": "./lib/core/cli.js"
    },
    "files": [
        "lib"
    ],
    "scripts":
    {
        "cz": "git add . && git cz",
        "log": "changelog-sn -i CHANGELOG.md -s -r 0",
        "log:all": "conventional-changelog --config lib/all -i CHANGELOG.md -s -u -r 0",
        "v": "standard-version"
    },
    "keywords": [
        "changelog",
        "git"
    ],
    "config":
    {
        "commitizen":
        {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable":
        {
            "config": ".cz-config.js"
        }
    },
    "husky":
    {
        "hooks":
        {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies":
    {
        "chalk": "^2.4.2",
        "compare-func": "^1.3.2",
        "longest": "^2.0.1",
        "q": "^1.5.1"
    },
    "devDependencies":
    {
        "add-stream": "^1.0.0",
        "commitizen": "^4.0.3",
        "commitlint": "^8.2.0",
        "cz-customizable": "^6.2.0",
        "github-url-from-git": "^1.5.0",
        "husky": "^3.0.4",
        "standard-version": "^7.0.0",
        "tempfile": "^3.0.0"
    },
    "repository":
    {
        "type": "git",
        "url": "https://github.com/WenHaoHuang/changelog-sn"
    },
    "license": "MIT"
}
